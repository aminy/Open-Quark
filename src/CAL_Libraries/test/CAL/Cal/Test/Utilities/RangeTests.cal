testIntersectWholeSpace =
let
    wholeSpace :: Range Double;
    wholeSpace = makeEntireRange;
in
((intersectTwoRanges wholeSpace (makeBetweenIncludingEndpointsRange 1.0 2.0) == Just (makeBetweenIncludingEndpointsRange 1.0 2.0)) || error "RangeTests: Failed on intersect whole test 0") &&
((intersectTwoRanges wholeSpace (makeBetweenIncludingLeftEndpointRange 1.0 2.0) == Just (makeBetweenIncludingLeftEndpointRange 1.0 2.0)) || error "RangeTests: Failed on intersect whole test 1") &&
((intersectTwoRanges wholeSpace (makeBetweenIncludingRightEndpointRange 1.0 2.0) == Just (makeBetweenIncludingRightEndpointRange 1.0 2.0)) || error "RangeTests: Failed on intersect whole test 2") &&
((intersectTwoRanges wholeSpace (makeBetweenExcludingEndpointsRange 1.0 2.0) == Just (makeBetweenExcludingEndpointsRange 1.0 2.0)) || error "RangeTests: Failed on intersect whole test 3") &&
((intersectTwoRanges wholeSpace (makeIsLessThanEqualsRange  1.0) == Just (makeIsLessThanEqualsRange  1.0)) || error "RangeTests: Failed on intersect whole test 4") &&
((intersectTwoRanges wholeSpace (makeIsLessThanRange  1.0) == Just (makeIsLessThanRange  1.0)) || error "RangeTests: Failed on intersect whole test 5") &&
((intersectTwoRanges wholeSpace (makeIsGreaterThanRange 1.0 ) == Just (makeIsGreaterThanRange 1.0 )) || error "RangeTests: Failed on intersect whole test 6") &&
((intersectTwoRanges wholeSpace (makeIsGreaterThanEqualsRange 1.0 ) == Just (makeIsGreaterThanEqualsRange 1.0 )) || error "RangeTests: Failed on intersect whole test 7") &&
((intersectTwoRanges wholeSpace wholeSpace == Just wholeSpace) || error "RangeTests: Failed on intersect whole test 8") 
;

