// -------------- PERFORMANCE TESTING FUNCTIONS ------------------------------------

makeNonOverlappingRanges :: Double -> Int -> [Range Double];
makeNonOverlappingRanges start count = 
    take count (map (\ a -> makeBetweenIncludingEndpointsRange (2*(a-1)) (2*a-1.0)) (upFrom start));

makeOverlappingRanges :: Double -> Int -> [Range Double];
makeOverlappingRanges start count = 
    take count (map (\ a -> makeBetweenIncludingEndpointsRange 1.0 (2*a-1.0)) (upFrom start));

// O(n) count to ensure all elements of list are generated (anti-laziness)
count :: [a] -> Int;
count rl =
    case rl of
    [] -> 0;
    r1 : rs -> 1 + count rs;
    ;

// Performance testers for intersecting n non/overlapping ranges.
pTestIntersectO  n = (intersectRanges (makeOverlappingRanges 1.0 n)) == Nothing;
pTestIntersectNO n = (intersectRanges (makeNonOverlappingRanges 1.0 n)) == Nothing;

// Performance testers for consolidating n non/overlapping ranges
pTestConsolidateO  n = count (consolidateRanges (makeOverlappingRanges 1.0 n));
pTestConsolidateNO n = count (consolidateRanges (makeNonOverlappingRanges 1.0 n));

// Performance testers for unifying n and m non/overlapping ranges
pTestUnionNO n m = count (unionRanges (makeNonOverlappingRanges 1.0 n) (makeNonOverlappingRanges 10001.0 m));
pTestUnionO  n m = count (unionRanges (makeOverlappingRanges 1.0 n) (makeOverlappingRanges 1.5 m));

// Performance testers for differencing n against m non/overlapping ranges
pTestDifferenceNO n m = count (unionRanges (makeNonOverlappingRanges 1.0 n) (makeNonOverlappingRanges 10001.0 m));
pTestDifferenceO2 n m = count (unionRanges (makeNonOverlappingRanges 1.0 n) (makeNonOverlappingRanges 1.5 m));
pTestDifferenceO1 n   = count (differenceRanges [makeBetweenIncludingEndpointsRange 1.0 10001.0] (makeNonOverlappingRanges 1.5 n));
